{
	"name": "front-comp",
	"version": "0.0.1",
	"scripts": {
		"ng": "ng",
		"start:dev": "rimraf dist && ng serve",
		"build": "ng build --configuration production",
		"build:prod": "ng build --configuration production && npm run removecss",
		"removecss": "cd dist/FrontComp && purgecss --css ./styles.css --content *.html *.js --output ./styles.css",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"compodoc": "npx compodoc -p tsconfig.json -s",
		"commit": "cz",
		"webpack-bundle-analyzer": "webpack-bundle-analyzer dist/FrontComp/stats.json"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^12.2.5",
		"@angular/cdk": "^12.2.5",
		"@angular/common": "^12.2.5",
		"@angular/compiler": "^12.2.5",
		"@angular/core": "^12.2.5",
		"@angular/forms": "^12.2.5",
		"@angular/material": "^12.2.5",
		"@angular/platform-browser": "^12.2.5",
		"@angular/platform-browser-dynamic": "^12.2.5",
		"@angular/router": "^12.2.5",
		"@auth0/angular-jwt": "^5.0.2",
		"@swimlane/ngx-charts": "^19.0.1",
		"aos": "^2.3.4",
		"bootstrap": "^5.1.1",
		"d3": "^7.0.1",
		"date-fns": "^2.23.0",
		"flexsearch": "^0.7.21",
		"js-cookie": "^2.2.1",
		"lodash": "^4.17.21",
		"ng-sidebar": "^9.4.2",
		"ngx-logger": "^4.3.3",
